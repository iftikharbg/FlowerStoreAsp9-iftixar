@model Category


    <div class="content-wrapper">
        <div class="row">
            <div class="col-md-6 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">@Model.Name</h4>
                        <p class="card-description">
                            @Html.Raw(Model.Desc)
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>

